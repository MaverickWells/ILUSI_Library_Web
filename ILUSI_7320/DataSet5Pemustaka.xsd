                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (5) TBL_Pemustaka.nama_pemustaka AS 'Nama Dosen', TBL_Pemustaka.no_anggota AS NPP, COUNT(TBL_Transaksi.no_anggota) AS 'Jumlah Peminjaman'
FROM            TBL_Detil_Transaksi INNER JOIN
                         TBL_Transaksi ON TBL_Detil_Transaksi.id_transaksi = TBL_Transaksi.id_transaksi INNER JOIN
                         TBL_Pemustaka ON TBL_Transaksi.no_anggota = TBL_Pemustaka.no_anggota INNER JOIN
                         TBL_Prodi ON TBL_Pemustaka.kode_prodi = TBL_Prodi.kode_prodi
WHERE        (TBL_Pemustaka.kode_katagori = 1) AND (TBL_Transaksi.tanggal_pinjam BETWEEN CONVERT(DATE, @tglStart) AND CONVERT(DATE, @tglStop)) AND (TBL_Pemustaka.kode_prodi = @kodeProdi)
GROUP BY TBL_Pemustaka.nama_pemustaka, TBL_Pemustaka.no_anggota
ORDER BY 'Jumlah Peminjaman' DESC</CommandText>
