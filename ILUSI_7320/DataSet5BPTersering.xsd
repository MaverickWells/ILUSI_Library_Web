                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TOP (5) TBL_Bahan_Pustaka.judul_buku AS 'Judul Buku', TBL_Bahan_Pustaka.pengarang_buku AS Pengarang, TBL_Bahan_Pustaka.penerbit_buku AS Penerbit, COUNT(TBL_Detil_Transaksi.no_katalog) 
                         AS 'Jumlah Peminjaman'
FROM            TBL_Detil_Transaksi INNER JOIN
                         TBL_Bahan_Pustaka ON TBL_Detil_Transaksi.no_katalog = TBL_Bahan_Pustaka.no_katalog INNER JOIN
                         TBL_Prodi ON TBL_Bahan_Pustaka.kode_prodi = TBL_Prodi.kode_prodi INNER JOIN
                         TBL_Transaksi ON TBL_Detil_Transaksi.id_transaksi = TBL_Transaksi.id_transaksi
WHERE        (TBL_Prodi.kode_prodi = @kode_prodi) AND (TBL_Transaksi.tanggal_pinjam BETWEEN CONVERT(DATE, @tglStart) AND CONVERT(DATE, @tglStop))
GROUP BY TBL_Bahan_Pustaka.judul_buku, TBL_Bahan_Pustaka.pengarang_buku, TBL_Bahan_Pustaka.penerbit_buku
ORDER BY 'Jumlah Peminjaman' DESC</CommandText>
