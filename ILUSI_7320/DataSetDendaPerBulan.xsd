                      <Parameter AllowDbNull="true" AutogeneratedName="tglStart" ColumnName="kode_bulan" DataSourceName="ILUSI_7320.dbo.TBL_Bulan" DataTypeServer="varchar(50)" DbType="AnsiString" Direction="Input" ParameterName="@tglStart" Precision="0" ProviderType="VarChar" Scale="0" Size="50" SourceColumn="kode_bulan" SourceColumnNullMapping="false" SourceVersion="Current" />
