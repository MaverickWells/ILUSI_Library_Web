              <DbSource ConnectionRef="ILUSI_7320ConnectionString (Web.config)" DbObjectName="ILUSI_7320.dbo.TBL_Bulan" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetDataDendaPerBulan" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetDataDendaPerBulan" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetDataDendaPerBulan" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TBL_Bulan.nama_bulan AS Bulan, ISNULL(SUM(TBL_Detil_Transaksi.denda), 0) AS 'Jumlah Denda'
FROM            TBL_Detil_Transaksi INNER JOIN
                         TBL_Bahan_Pustaka ON TBL_Detil_Transaksi.no_katalog = TBL_Bahan_Pustaka.no_katalog INNER JOIN
                         TBL_Prodi ON TBL_Bahan_Pustaka.kode_prodi = TBL_Prodi.kode_prodi INNER JOIN
                         TBL_Transaksi ON TBL_Detil_Transaksi.id_transaksi = TBL_Transaksi.id_transaksi RIGHT OUTER JOIN
                         TBL_Bulan ON MONTH(TBL_Detil_Transaksi.tgl_kembali_aktual) = TBL_Bulan.kode_bulan
WHERE        (TBL_Bulan.kode_bulan BETWEEN MONTH(CONVERT(DATE, @tglStart)) AND MONTH(CONVERT(DATE, @tglStop)))
GROUP BY TBL_Bulan.nama_bulan, TBL_Bulan.kode_bulan
ORDER BY TBL_Bulan.kode_bulan</CommandText>
