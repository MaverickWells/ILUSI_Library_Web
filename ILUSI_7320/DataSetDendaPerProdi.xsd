                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        TBL_Prodi.nama_prodi AS Prodi, ISNULL(SUM(TBL_Detil_Transaksi.denda), 0) AS 'Jumlah Denda'
FROM            TBL_Detil_Transaksi INNER JOIN
                         TBL_Bahan_Pustaka ON TBL_Detil_Transaksi.no_katalog = TBL_Bahan_Pustaka.no_katalog RIGHT OUTER JOIN
                         TBL_Prodi ON TBL_Bahan_Pustaka.kode_prodi = TBL_Prodi.kode_prodi
WHERE         (TBL_Detil_Transaksi.tgl_kembali_aktual BETWEEN CONVERT(DATE, @tglStart) AND CONVERT(DATE, @tglStop)) OR
                         (TBL_Detil_Transaksi.tgl_kembali_aktual IS NULL)
GROUP BY TBL_Prodi.nama_prodi, TBL_Prodi.kode_prodi
ORDER BY TBL_Prodi.kode_prodi</CommandText>
