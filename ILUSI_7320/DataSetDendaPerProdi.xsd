                      <Parameter AllowDbNull="true" AutogeneratedName="tglStart" ColumnName="tgl_kembali_aktual" DataSourceName="ILUSI_7320.dbo.TBL_Detil_Transaksi" DataTypeServer="date" DbType="AnsiString" Direction="Input" ParameterName="@tglStart" Precision="0" ProviderType="Date" Scale="0" Size="3" SourceColumn="tgl_kembali_aktual" SourceColumnNullMapping="false" SourceVersion="Current" />
